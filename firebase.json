{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "proxy-server/**",
      "README.md"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "health-app-proxy",
          "region": "asia-southeast1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
